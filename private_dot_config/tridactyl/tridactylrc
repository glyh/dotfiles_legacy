colors --url https://raw.fastgit.org/bezmi/base16-tridactyl/master/base16-nord.css nord

set smoothscroll true

bind / fillcmdline find
bind ? fillcmdline find -?
bind n findnext 1
bind N findnext -1
bind ,<Space> nohlsearch

bind s scrollpage 0.5
bind d scrollpage -0.5
bind x tabclose
bind a hint -b
bind m bmark
bind h tabprev
bind l tabnext
